<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="rat" class="sqlcmd.spring.LabRat">
        <constructor-arg value="Hi!"/>
        <property name="name" value="Alice"/>
    </bean>

    <bean id="clear" class="sqlcmd.ClearAllData">
    </bean>

    <bean id="tableNames" class="sqlcmd.command.table.TableNames">
    </bean>
    <bean id="tableSize" class="sqlcmd.command.table.TableSize">
    </bean>
    <bean id="tableCreate" class="sqlcmd.command.table.TableCreate">
    </bean>
</beans>